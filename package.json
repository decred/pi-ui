    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-testing-library": "^3.9.0",
    "fs-copy-file-sync": "^1.1.1",
    "gh-pages": "^3.2.3",
    "identity-obj-proxy": "^3.0.0",
